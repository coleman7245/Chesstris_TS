{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    "module": "es2022",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "jsx" : "react-jsx"
  }
}
